@model IEnumerable<PayPal.Api.Item>
@{
    ViewBag.Title = "AddToCart";
}

<h2>AddToCart</h2>
@if (Model == null)
{
    <p>Fill Cart So i Can Get To 50.000</p>
}
else
{

    foreach (var item in Model)
    {
        <p>Name: @item.name   Price:  @item.price   Tax: @item.tax  Quantity:  @item.quantity</p>
    }



    <a href="@Url.Action("PayPalPayment", "PayPal")"><img src="~/images/cart.png" /> Puchase </a>

}
