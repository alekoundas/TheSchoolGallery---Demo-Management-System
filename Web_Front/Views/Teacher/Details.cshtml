@model Web_DomainClasses.Entities.School.Teacher

@{
    ViewBag.Title = "Details";
}

<div class="limiter">
    <div class="container-login100">
        <div class="container">
            <br />
            <h2 class="text-center" style="color:#f3efe9;"> Teacher in Detail</h2>
            <hr />
            <br />
            <div class="row">
                <!-- Section - School Details ---------------------------------------->
                <div class="col-md-12 mx-auto text-center">
                    <div class="paintingcontainer">
                        <div class="Schooldesc">
                            <h3>@Html.DisplayFor(model => model.FirstName) @Html.DisplayFor(model => model.LastName)</h3>
                            <hr />

                            <h4>
                                @Html.DisplayNameFor(model => model.Classrooms): @Html.DisplayFor(model => model.Classrooms)
                            </h4>
                        </div>



                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

